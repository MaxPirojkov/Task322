    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textViewNumb1"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@color/colorTextViewNumb"
            android:gravity="right"
            android:textColor="@color/colorTextInVeiwNumb"
            android:textSize="75sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_MC"
                style="@style/calc_button1"
                android:text="MC" />

            <Button
                android:id="@+id/btn_reset1"
                style="@style/calc_button1"
                android:text="C" />


            <Button
                android:id="@+id/btn_plus_minus1"
                style="@style/calc_button1"
                android:text="CE" />

            <Button
                android:id="@+id/btn_persent1"
                style="@style/calc_button1"
                android:text="%" />



            <Button
                android:id="@+id/btn_division1"
                style="@style/btn_orange1"
                android:text="÷"/>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_MR"
                style="@style/calc_button1"
                android:text="MR" />

            <Button
                android:id="@+id/btn_seven1"
                style="@style/calc_button1"
                android:text="7" />

            <Button
                android:id="@+id/btn_eight1"
                style="@style/calc_button1"
                android:text="8" />

            <Button
                android:id="@+id/btn_nine1"
                style="@style/calc_button1"
                android:text="9" />

            <Button
                android:id="@+id/btnMultip1"
                style="@style/btn_orange1"
                android:text="×" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_MS"
                style="@style/calc_button1"
                android:text="MS" />

            <Button
                android:id="@+id/btn_four1"
                style="@style/calc_button1"
                android:text="4" />

            <Button
                android:id="@+id/btn_five1"
                style="@style/calc_button1"
                android:text="5" />

            <Button
                android:id="@+id/btn_six1"
                style="@style/calc_button1"
                android:text="6" />

            <Button
                android:id="@+id/btnMinus1"
                style="@style/btn_orange1"
                android:text="-" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_MPlus"
                style="@style/calc_button1"
                android:text="M+" />

            <Button
                android:id="@+id/btn_one1"
                style="@style/calc_button1"
                android:text="1" />

            <Button
                android:id="@+id/btn_two1"
                style="@style/calc_button1"
                android:text="2" />

            <Button
                android:id="@+id/btn_three1"
                style="@style/calc_button1"
                android:text="3" />

            <Button
                android:id="@+id/btnPlus1"
                style="@style/btn_orange1"
                android:text="+"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_MMin"
                style="@style/calc_button1"
                android:text="M-" />

            <Button
                android:id="@+id/btn_zero1"
                style="@style/calc_button1"
                android:layout_weight="1.15"
                android:text="0" />

            <Button
                android:id="@+id/btn_dot1"
                style="@style/calc_button1"
                android:text="."
                android:textStyle="bold"/>

            <Button
                android:id="@+id/btn_equally1"
                style="@style/btn_orange1"
                android:text="=" />
        </LinearLayout>

        <Button
            android:id="@+id/changeMode11"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:text="⦿" />
    </LinearLayout>




</FrameLayout>




